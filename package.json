{
  "name": "nuxt-starter-template",
  "version": "1.0.0",
  "description": "A Nuxt starter template.",
  "author": {
    "name": "elenh",
    "email": "yisiwings@163.com"
  },
  "keywords": [
    "vue",
    "nuxt",
    "egg",
    "starter template",
    "ssr",
    "vuetify"
  ],
  "scripts": {
    "dev": "egg-bin dev",
    "build": "npm stop && npm start",
    "start": "egg-scripts start --daemon --title=nuxt-starter-template",
    "stop": "egg-scripts stop --title=nuxt-starter-template",
    "build:ft": "cross-env NUXT_ENV_APP=ft nuxt build",
    "build:prod": "cross-env NUXT_ENV_APP=production nuxt build",
    "nuxt:start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "test": "jest"
  },
  "dependencies": {
    "@aidol/svg-icon": "^1.2.0",
    "@nuxtjs/axios": "^5.12.2",
    "core-js": "^3.6.5",
    "egg": "^2.29.1",
    "egg-cors": "^2.2.3",
    "egg-scripts": "^2.13.0",
    "nuxt": "^2.14.6",
    "vue-toasted": "^1.1.28"
  },
  "devDependencies": {
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.0",
    "cross-env": "^7.0.2",
    "egg-bin": "^4.15.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.5.0",
    "prettier": "^2.1.2",
    "qs": "^6.9.4",
    "svg-sprite-loader": "^5.0.0",
    "vue-jest": "^3.0.4"
  },
  "engines": {
    "node": ">= 10.x"
  }
}
